{
  "name": "nexi-checkout/oxid7",
  "description": "Nexi Checkout payment module for Oxid 7",
  "version": "1.0.2",
  "type": "oxideshop-module",
  "keywords": [
    "oxid",
    "modules",
    "eShop"
  ],
  "license": [
    "AGPL-3.0-or-later"
  ],
  "prefer-stable": true,
  "prefer-dist": true,
  "require": {
    "php": "^8.0 | ^8.1"
  },
  "minimum-stability": "dev",
  "conflict": {
    "oxid-esales/oxideshop-ce": "<7.0"
  },
  "autoload": {
    "psr-4": {
      "NexiCheckout\\": "./"
    }
  },
  "config": {
    "allow-plugins": {
      "oxid-esales/oxideshop-composer-plugin": true,
      "oxid-esales/oxideshop-unified-namespace-generator": true
    }
  }
}

